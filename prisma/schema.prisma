generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("POSTGRES_URL")
}

model Message {
  id        Int      @id @default(autoincrement())
  name      String
  mobile_num String
  email     String
  company   String
  message   String
  createdAt DateTime @default(now())
}

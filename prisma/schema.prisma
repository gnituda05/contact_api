generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "rhel-openssl-1.0.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("POSTGRES_URL")
}

model Message {
  id        Int      @id @default(autoincrement())
  name      String
  mobile_num String
  email     email
  company   String
  message   String
  createdAt DateTime @default(now())
}
